import { Player } from './player';

export class Quiz{

    public id : number;
    public playerOne: Player;
    private playerTwo: Player;
    private quizRounds: QuizRound[];


    id: number;
    question: string;
    answer: string;
    options: string[];
    category: string;
    difficulty: string;
    type: string;
    correct: boolean;
    selected: boolean;
    constructor(){
        this.id = 0;
        this.question = '';
        this.answer = '';
        this.options = [];
        this.category = '';
        this.difficulty = '';
        this.type = '';
        this.correct = false;
        this.selected = false;
    }
}